{
  "name": "hexadash-nextjs",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16.8.0"
  },
  "main": "main.js",
  "author": "RBFraphael",
  "description": "Electron + NextJS example project",
  "homepage": "./",
  "scripts": {
    "dev": "concurrently \"next dev\" \"npm run electron-dev\"",
    "electron-dev": "wait-on http://localhost:3000 && electron .",
    "build": "next build",
    "package": "electron-builder -w dir --x64",
    "renderer": "next build .",
    "start": "next start",
    "start-electron": "electron ."
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/pro-card": "^2.5.13",
    "@ant-design/pro-layout": "^7.19.11",
    "@ant-design/pro-table": "^3.11.1",
    "@auth0/auth0-react": "^2.2.1",
    "@auth0/nextjs-auth0": "^3.1.0",
    "@iconscout/react-unicons": "^2.0.2",
    "@prisma/client": "^5.22.0",
    "@react-google-maps/api": "^2.19.2",
    "@redux-devtools/extension": "^3.2.5",
    "@reduxjs/toolkit": "^2.3.0",
    "@tinymce/tinymce-react": "^4.3.0",
    "antd": "^5.9.4",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "build": "^0.1.4",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.9",
    "electron-serve": "^2.1.1",
    "emoji-picker-react": "^4.4.11",
    "firebase": "^10.3.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "i18next": "^23.4.6",
    "instagram-private-api": "^1.46.1",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "leaflet": "^1.9.4",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.29.4",
    "next": "^13.5.7",
    "next-connect": "^1.0.0",
    "next-redux-wrapper": "^8.1.0",
    "peity-react": "^0.0.7",
    "postcss": "8.4.28",
    "prisma": "^5.22.0",
    "puppeteer": "^23.6.0",
    "rc-tween-one": "^3.0.6",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-countdown": "^2.3.5",
    "react-countup": "^6.4.2",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-fontawesome": "^1.7.1",
    "react-google-charts": "^4.0.1",
    "react-i18next": "^13.2.0",
    "react-id-swiper": "^4.0.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-cluster": "^2.1.0",
    "react-lightbox-pack": "^0.2.1",
    "react-masonry-css": "^1.0.16",
    "react-modal-video": "^2.0.1",
    "react-redux": "^8.1.3",
    "react-responsive-masonry": "^2.4.1",
    "react-router-dom": "^6.15.0",
    "react-simple-maps": "^3.0.0",
    "react-svg": "^16.1.22",
    "react-tagsinput": "^3.20.3",
    "react-tooltip": "^5.21.1",
    "recharts": "^2.8.0",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "sharp": "^0.33.5",
    "styled-components": "^6.0.7",
    "swiper": "^10.2.0",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "video-snapshot": "^1.0.11"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/js-cookie": "^3.0.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react-fontawesome": "^1.6.8",
    "@types/react-modal-video": "^1.2.3",
    "@types/react-responsive-masonry": "^2.1.3",
    "concurrently": "^9.0.1",
    "electron": "^25.9.8",
    "electron-builder": "^25.1.8",
    "eslint": "^9.15.0",
    "wait-on": "^8.0.1"
  },
  "build": {
    "appId": "instagram private",
    "includeSubNodeModules": "true",
    "files": [
      "next.config.js",
      "main.js",
      "out/**/*",
      "main/**",
      "preload.js"
    ],
    "extraResources": [
      "prisma/**/*",
      "node_modules/.prisma/**/*",
      "node_modules/@prisma/client/**/*"
    ],
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "nsis",
      "forceCodeSigning": false,
      "signAndEditExecutable": false
    }
  }
}
