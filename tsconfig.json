// @ts-ignore
{
  "compilerOptions": {
    "types": ["node"],
    "target": "es2016",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "esModuleInterop": true,
    "module": "CommonJS",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    "outDir": "../main",

  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "custom.d.ts", "src/layout/Style.tsx", "apalah/renderer.js"],
  "exclude": ["node_modules"]
}
